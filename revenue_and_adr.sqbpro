tab_browse><tab_sql><sql name="SQL 1">-- Focused on Revenue and Average Day Rate (ADR) --

-- Estimated Revenue per Hotel Type --

SELECT 
  hotels.hotel_type,
  ROUND(AVG(bookings.adr * (stay_details.stays_in_weekend_nights + stay_details.stays_in_week_nights)), 2) AS avg_revenue_per_booking,
  COUNT(*) AS total_bookings
FROM bookings
JOIN stay_details ON bookings.stay_id = stay_details.stay_id
JOIN hotels ON bookings.hotel_id = hotels.hotel_id
WHERE bookings.is_canceled = 0
GROUP BY hotels.hotel_type
ORDER BY avg_revenue_per_booking DESC;

-- Estimated Revenue by Market Segment --

SELECT 
  segments.market_segment,
  COUNT(*) AS total_bookings,
  ROUND(SUM(adr * (stay_details.stays_in_weekend_nights + stay_details.stays_in_week_nights)), 2) AS segment_revenue
FROM bookings
JOIN segments ON bookings.segment_id = segments.segment_id
JOIN stay_details ON bookings.stay_id = stay_details.stay_id
WHERE bookings.is_canceled = 0
GROUP BY segments.market_segment
ORDER BY segment_revenue DESC;

-- ADR Trends by Month --

SELECT
  stay_details.arrival_date_month,
  stay_details.arrival_date_year,
  COUNT(*) AS total_bookings,
  ROUND(AVG(bookings.adr), 2) AS avg_adr
FROM bookings
JOIN stay_details ON bookings.stay_id = stay_details.stay_id
WHERE bookings.is_canceled = 0
GROUP BY stay_details.arrival_date_year, stay_details.arrival_date_month
ORDER BY stay_details.arrival_date_year, 
         CASE stay_details.arrival_date_month
           WHEN 'January' THEN 1
           WHEN 'February' THEN 2
           WHEN 'March' THEN 3
           WHEN 'April' THEN 4
           WHEN 'May' THEN 5
           WHEN 'June' THEN 6
           WHEN 'July' THEN 7
           WHEN 'August' THEN 8
           WHEN 'September' THEN 9
           WHEN 'October' THEN 10
           WHEN 'November' THEN 11
           WHEN 'December' THEN 12
         END;

-- ADR by Hotel Type and Customer Type --

 SELECT
	hotels.hotel_type,
	guests.customer_type,
	ROUND(AVG(bookings.adr), 2) AS avg_adr,
	COUNT(*) AS total_bookings
FROM bookings
JOIN hotels ON bookings.hotel_id = hotels.hotel_id
JOIN guests ON bookings.guest_id = guests.guest_id
WHERE bookings.is_canceled = 0
GROUP BY hotels.hotel_type, guests.customer_type
ORDER BY avg_adr DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
