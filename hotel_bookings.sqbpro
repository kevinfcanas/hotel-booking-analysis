 1">-- Focused on Booking Outcomes and Cancellations --

-- Cancellation Rate by Hotel Type --

SELECT SUM(bookings.is_canceled) * 1.0 /  COUNT(*) AS cancellation_rate, hotels.hotel_type
FROM bookings
INNER JOIN hotels ON bookings.hotel_id = hotels.hotel_id
GROUP BY hotels.hotel_type;

-- Cancellation Rate by Hotel Type and Season --

SELECT stay_details.arrival_date_month, ROUND(SUM(bookings.is_canceled) * 1.0 /  COUNT(*) , 2)AS cancellation_rate, hotels.hotel_type
FROM bookings
INNER JOIN hotels ON bookings.hotel_id = hotels.hotel_id
INNER JOIN stay_details ON bookings.stay_id = stay_details.stay_id
GROUP BY hotels.hotel_type, stay_details.arrival_date_month
ORDER BY stay_details.arrival_date_month ASC;

-- Cancellation Rate by Market Segment and Distribution Channel --

SELECT segments.market_segment, ROUND(SUM(bookings.is_canceled) * 1.0 /  COUNT() , 2)AS cancellation_rate, segments.distribution_channel, COUNT() as total_bookings
FROM bookings
INNER JOIN segments ON bookings.segment_id= segments.segment_id
GROUP BY segments.market_segment, segments.distribution_channel
ORDER BY cancellation_rate DESC;

-- Cancellation Rate by Guest Composition --

SELECT
	COUNT(*) AS total_bookings,
	ROUND(SUM(bookings.is_canceled) * 1.0/COUNT(), 2) AS cancellation_likelihood,
	ROUND(AVG(stay_details.stays_in_week_nights + stay_details.stays_in_weekend_nights), 2) AS avg_stay_duration,
	CASE
		WHEN guests.adults = 1 AND guests.children = 0 AND guests.babies = 0 THEN &quot;solo&quot;
		ELSE &quot;family&quot;
	END  AS guest_type
FROM bookings
JOIN guests ON bookings.guest_id = guests.guest_id
JOIN stay_details ON bookings.stay_id = stay_details.stay_id
GROUP BY
	CASE
		WHEN guests.adults = 1 AND guests.children = 0 AND guests.babies = 0 THEN &quot;solo&quot;
	ELSE &quot;family&quot;
	END;

-- Cancellation Rate by Lead Time --

SELECT
	COUNT(*) AS total_bookings,
	ROUND(SUM(bookings.is_canceled) * 1.0/COUNT(), 2) AS cancellation_rate,
	CASE
		WHEN bookings.lead_time &lt; 30 THEN  &quot;short&quot;
		WHEN bookings.lead_time BETWEEN 30 AND 90 THEN &quot;medium&quot;
		ELSE &quot;long&quot;
	END AS lead_time_bucket,
ROUND(AVG(lead_time), 2) AS avg_lead_time
FROM bookings
GROUP BY
	CASE
		WHEN bookings.lead_time &lt; 30 THEN  &quot;short&quot;
		WHEN bookings.lead_time BETWEEN 30 AND 90 THEN &quot;medium&quot;
		ELSE &quot;long&quot;
	END
ORDER BY cancellation_rate DESC;

-- Cancellation Rate by Customer Type --

SELECT
	COUNT(*) AS total_bookings,
	ROUND(SUM(bookings.is_canceled) * 1.0/COUNT(), 2) AS cancellation_rate,
	guests.customer_type
FROM bookings
JOIN guests ON bookings.guest_id = guests.guest_id
GROUP BY guests.customer_type
ORDER BY cancellation_rate DESC;</sql><current_tab id="0"/></tab_sql></sqlb_project>
